/AddRoman-8 {
	roman-8-dict begin
		/roman-8-mappings exch def
		/newName exch def
		/oldName exch def
		/oldDict oldName findfont def
		/newDict oldDict maxlength dict def
		oldDict {
			exch dup /FID ne
			{
				dup /Encoding eq
				{
					exch dup length array copy
					newDict 3 1 roll put
				}
				{
					exch newDict 3 1 roll put
				} ifelse
			}
			{
				pop pop
			} ifelse
		} forall
		newDict /FontName newName put
		0 2 roman-8-mappings length 1 sub {
			dup
			roman-8-mappings exch get
			exch 1 add roman-8-mappings exch get
			newDict /Encoding get 3 1 roll put
		} for
		newName newDict definefont pop
	end
} bind def

/findfont-8 {
	/ofnamestr exch def
	/ofnmlg ofnamestr length def
	/nfnamestr ofnmlg 2 add string def
	ofnamestr (Symbol) eq
	{ ofnamestr findfont } 
	{
	ofnamestr nfnamestr copy pop
	nfnamestr ofnmlg (-8) putinterval
	ofnamestr cvn nfnamestr cvn roman-8-mappings AddRoman-8
	nfnamestr cvn findfont
	}
	ifelse
} def

