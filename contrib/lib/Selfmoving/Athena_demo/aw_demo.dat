*HrSelfMoving.grid : 10
!*HrSelfMoving.installAccelerators : false
!*HrSelfMoving.active : false


*mbutton.x : 20
*mbutton.y : 450
*mbutton.label : Main menu
*mbutton.font : *-Helvetica-*-O-*-180-*

*selftitle.x : 100
*selftitle.y : 10

*titlelabel.label : Selfmoving demo! 
*titlelabel.font : *-Helvetica-*-O-*-240-*
*selftitle.selectInReverseVideo : on
*titlelabel.borderWidth : 0
