*selflabel2.DiagonalHandle : NO_HANDLE
*selflabel2.borderWidth : 0
*selflabel2.showHorizontalLine : true
*label2.label : Surname
*label2.borderWidth : 0

*selflabel3.x : 50
*selflabel3.y : 200
*selflabel3.VerticalHandle: NO_HANDLE
*selflabel3.HorizontalHandle: NO_HANDLE
*selflabel3.DiagonalHandle : NO_HANDLE
*selflabel3.borderWidth : 0
*selflabel3.showHorizontalLine : true
*label3.label : City
*label3.borderWidth : 0

*selftext1.x : 120
*selftext1.y : 100
*selftext1.showHorizontalLine : true
*text1*editType : edit
*text1*string : "Please, edit me..."
*text1.width : 200

*selftext2.x : 120
*selftext2.y : 150
*selftext2.showHorizontalLine : true
*text2*editType : edit
*text2*string : "Please, edit me..."
