# #include <Library.tmpl>

CSRCS = \
	XpexInfo.c \
	XpexLut.c \
	XpexPC.c \
	XpexRenderers.c \
	XpexStr.c \
	XpexNameSets.c \
	XpexSC.c \
	XpexPhigsWks.c \
	XpexPick.c \
	XpexFont.c \
	XpexlibInt.c

OBJS = \
	XpexInfo.o \
	XpexLut.o \
	XpexPC.o \
	XpexRenderers.o \
	XpexStr.o \
	XpexNameSets.o \
	XpexSC.o \
	XpexPhigsWks.o \
	XpexPick.o \
	XpexFont.o \
	XpexlibInt.o


SRCS = $(CSRCS)

.SUFFIXES: .o

 PEXAPIINC = -I../../include
PEXINCLUDE = -I$(EXTENSIONSRC)/include/PEX
PEXPRIVINC = -I../include
  INCLUDES = $(PEXPRIVINC) $(PEXAPIINC) $(PEXINCLUDE) \
	-I$(BUILDINCDIR) -I$(EXTENSIONSRC)/include

NormalRelocatableTarget(res,$(OBJS))
DependTarget()
extract : $(SRCS)
