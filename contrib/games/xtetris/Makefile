	# load $(ALLDEFINES) $(OBJS) $(LOCAL_LIBRARIES) $(SYS_LIBRARIES) $(EXTRA_LIBRARIES)

install:: xtetris
	@if [ -d $(DESTDIR)$(BINDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(BINDIR)); fi
	$(INSTALL) -c $(INSTPGMFLAGS)  xtetris $(DESTDIR)$(BINDIR)

install.man:: xtetris.man
	@if [ -d $(DESTDIR)$(MANDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(MANDIR)); fi
	$(INSTALL) -c $(INSTMANFLAGS) xtetris.man $(DESTDIR)$(MANDIR)/xtetris.$(MANSUFFIX)

depend::
	$(DEPEND) $(DEPENDFLAGS) -s "# DO NOT DELETE" -- $(ALLDEFINES) -- $(SRCS)

lint:
	$(LINT) $(LINTFLAGS) $(SRCS) $(LINTLIBS)
lint1:
	$(LINT) $(LINTFLAGS) $(FILE) $(LINTLIBS)

clean::
	$(RM) $(PROGRAM)

all:: Xtetris.ad

all:: Xtetris.c.ad

all:: Xtetris.bw.ad

install:: Xtetris.c.ad
	@if [ -d $(DESTDIR)$(XAPPLOADDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(XAPPLOADDIR)); fi
	$(INSTALL) -c $(INSTAPPFLAGS) Xtetris.c.ad $(DESTDIR)$(XAPPLOADDIR)/Xtetris.c

install:: Xtetris.bw.ad
	@if [ -d $(DESTDIR)$(XAPPLOADDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(XAPPLOADDIR)); fi
	$(INSTALL) -c $(INSTAPPFLAGS) Xtetris.bw.ad $(DESTDIR)$(XAPPLOADDIR)/Xtetris.bw

install:: Xtetris.ad
	@if [ -d $(DESTDIR)$(XAPPLOADDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(XAPPLOADDIR)); fi
	$(INSTALL) -c $(INSTAPPFLAGS) Xtetris.ad $(DESTDIR)$(XAPPLOADDIR)/Xtetris

install:: $(BITMAPS)
	@if [ -d $(DESTDIR)$(INCDIR)/bitmaps ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(INCDIR)/bitmaps); fi
	@case '${MFLAGS}' in *[i]*) set +e;; esac; \
	for i in $(BITMAPS); do \
	(set -x; $(INSTALL) -c -m 444 $$i $(DESTDIR)$(INCDIR)/bitmaps); \
	done

install:: xtetris-scores
	@if [ -d $(DESTDIR)$(LIBDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(LIBDIR)); fi
	@case '${MFLAGS}' in *[i]*) set +e;; esac; \
	for i in xtetris-scores; do \
	(set -x; $(INSTALL) -c $(SCOREFLAGS) $$i $(DESTDIR)$(LIBDIR)); \
	done
Xtetris.c.ad: Xtetris.ad.all
	xrdb -n Xtetris.ad.all -DCOLOR > Xtetris.c.ad
Xtetris.bw.ad: Xtetris.ad.all
	xrdb -n Xtetris.ad.all -UCOLOR > Xtetris.bw.ad
Xtetris.ad: Xtetris.ad.all
	xrdb -n Xtetris.ad.all -UCOLOR > Xtetris.ad
clean::
	$(RM) Xtetris.c.ad Xtetris.bw.ad Xtetris.ad

